# Copy to .env and fill values
GEMINI_API_KEY=your_gemini_api_key_here  # Get from https://makersuite.google.com/app/apikey

# ChromaDB persist directory
CHROMA_PERSIST_DIR=./chroma_data

# Model Configuration
GEMINI_LLM_MODEL=gemini-2.5-flash  # LLM model for text generation
USE_GEMINI_EMBEDDINGS=false         # Set to 'true' to use Gemini for embeddings (costs quota!)
GEMINI_EMBEDDING_MODEL=gemini-embedding-001  # Only used if USE_GEMINI_EMBEDDINGS=true
